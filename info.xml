<?xml version="1.0"?>
<extension key="uk.artfulrobot.civicrm.importhelper" type="module">
  <file>importhelper</file>
  <name>CSV Import Helper</name>
  <description>This tool helps you interactively assign CiviCRM contacts to
    your CSV file. This leaves you with a CSV file you can then import using
    CiviCRM's built-in import workflows, e.g. to Contributions or
    Activities. See documentation link.</description>
  <license>GPL-3.0+</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://artfulrobot.github.io/uk.artfulrobot.civicrm.importhelper/</url>
    <url desc="Documentation">https://artfulrobot.github.io/uk.artfulrobot.civicrm.importhelper/</url>
    <url desc="Support">https://artfulrobot.uk</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2017-01-27</releaseDate>
  <version>1.1.2-beta</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.7</ver>
    <ver>4.6</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/CsvImportHelper</namespace>
  </civix>
</extension>
